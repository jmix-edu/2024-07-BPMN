  <process id="service-tasks-demo" name="Service tasks demo" isExecutable="true" flowable:candidateStarterUsers="peter" jmix:candidateStarterUsersValue="peter">
    <startEvent id="startEvent1">
      <extensionElements>
        <jmix:formData type="input-dialog" openMode="DIALOG">
          <jmix:formFields>
            <jmix:formField id="message" caption="" type="string" editable="true" required="true" />
            <jmix:formField id="numberOfUsers" caption="" type="long" editable="true" required="false" />
          </jmix:formFields>
        </jmix:formData>
      </extensionElements>
      <outgoing>Flow_0wir0n1</outgoing>
    </startEvent>
    <serviceTask id="Activity_1wz5oqb" name="Java delegate" flowable:class="com.company.bpmdemo.delegate.RandomIndexJavaDelegate" jmix:taskType="javaDelegateClass">
      <incoming>Flow_0wir0n1</incoming>
      <outgoing>Flow_10h5o1m</outgoing>
    </serviceTask>
    <sequenceFlow id="Flow_10h5o1m" sourceRef="Activity_1wz5oqb" targetRef="Activity_1bd6i6k" />
    <serviceTask id="Activity_1bd6i6k" name="Java delegate with field injection" flowable:class="com.company.bpmdemo.delegate.UpperCaseJavaDelegate" jmix:taskType="javaDelegateClass">
      <extensionElements>
        <flowable:field name="decorationField">
          <flowable:string>***</flowable:string>
        </flowable:field>
        <flowable:field name="messageField">
          <flowable:expression>$(message)</flowable:expression>
        </flowable:field>
      </extensionElements>
      <incoming>Flow_10h5o1m</incoming>
      <outgoing>Flow_1nf6scs</outgoing>
    </serviceTask>
    <sequenceFlow id="Flow_1nf6scs" sourceRef="Activity_1bd6i6k" targetRef="Activity_1j494e8" />
    <serviceTask id="Activity_1j494e8" name="Java delegate expression" flowable:delegateExpression="${randomUserDelegate}" jmix:taskType="delegateExpression">
      <incoming>Flow_1nf6scs</incoming>
      <outgoing>Flow_1ovbhvp</outgoing>
    </serviceTask>
    <serviceTask id="Activity_10ysd5w" name="Spring bean task" flowable:expression="${greetingMessageBean.printMessage(&#39;Hello&#39;,message,execution)}" flowable:resultVariable="printedTime" jmix:taskType="springBean" jmix:beanName="greetingMessageBean">
      <extensionElements>
        <jmix:springBean beanName="greetingMessageBean" methodName="printMessage">
          <jmix:methodParam name="greeting" type="java.lang.String" isVariable="false">Hello</jmix:methodParam>
          <jmix:methodParam name="message" type="java.lang.String" isVariable="true">message</jmix:methodParam>
          <jmix:methodParam name="execution" type="org.flowable.engine.delegate.DelegateExecution" isVariable="true">execution</jmix:methodParam>
        </jmix:springBean>
      </extensionElements>
      <incoming>Flow_1ovbhvp</incoming>
      <outgoing>Flow_01140rh</outgoing>
    </serviceTask>
    <serviceTask id="Activity_0h4hbkg" name="Expression" flowable:expression="$(greetingMessageBean.printTime(printedTime))" flowable:resultVariable="timeInMillis" jmix:taskType="expression">
      <incoming>Flow_01140rh</incoming>
      <outgoing>Flow_0r9m7xv</outgoing>
    </serviceTask>
    <sequenceFlow id="Flow_0wir0n1" sourceRef="startEvent1" targetRef="Activity_1wz5oqb" />
    <sequenceFlow id="Flow_1ovbhvp" sourceRef="Activity_1j494e8" targetRef="Activity_10ysd5w" />
    <sequenceFlow id="Flow_01140rh" sourceRef="Activity_10ysd5w" targetRef="Activity_0h4hbkg" />
    <endEvent id="Event_0j3oh0c">
